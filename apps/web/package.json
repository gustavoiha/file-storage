{
  "name": "@dockspace/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.json --noEmit && vite build",
    "build:dev": "tsc -p tsconfig.json --noEmit && vite build --mode dev",
    "build:prod": "tsc -p tsconfig.json --noEmit && vite build --mode prod",
    "deploy:dev": "AWS_PROFILE=file-storage-dev ./scripts/deploy.sh dev",
    "deploy:prod": "AWS_PROFILE=file-storage-prod ./scripts/deploy.sh prod",
    "preview": "vite preview",
    "test": "vitest run",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.62.16",
    "@tanstack/react-router": "^1.89.6",
    "@tanstack/react-store": "^0.8.0",
    "@tanstack/store": "^0.8.0",
    "aws-amplify": "^6.13.6",
    "lucide-react": "^0.563.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "jsdom": "^25.0.1",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vitest": "^2.1.9"
  }
}
